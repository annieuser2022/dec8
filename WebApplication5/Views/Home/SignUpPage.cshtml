@{
    ViewData["Title"] = "Signup";
    @model WebApplication5.Models.User
        <form asp-action="SignupUser" method="post">
        Name:
        @Html.TextBoxFor(l=>l.Name, new {@class="form-control"})
        EmailID:
        @Html.TextBoxFor(l=>l.EmailId, new {@class="form-control"})
        Password:
        @Html.TextBoxFor(l=>l.Password, new {@class="form-control"})
        Role:
        @Html.TextBoxFor(l=>l.Role, new {@class="form-control"})
        <input type="submit" value="Submit"/>
        </form>
